{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/meme-social/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/meme-social/tsconfig.app.json"
  },
  "projects": {
    "api-getway": {
      "type": "application",
      "root": "apps/api-getway",
      "entryFile": "main",
      "sourceRoot": "apps/api-getway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-getway/tsconfig.app.json"
      }
    },
     "role": {
      "type": "application",
      "root": "apps/role",
      "entryFile": "main",
      "sourceRoot": "apps/role/src",
      "compilerOptions": {
        "tsConfigPath": "apps/role/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "data-service": {
      "type": "application",
      "root": "apps/data-service",
      "entryFile": "main",
      "sourceRoot": "apps/data-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/data-service/tsconfig.app.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "elastic-search-service": {
      "type": "application",
      "root": "apps/elastic-search-service",
      "entryFile": "main",
      "sourceRoot": "apps/elastic-search-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/elastic-search-service/tsconfig.app.json"
      }
    },
    "meme-social": {
      "type": "application",
      "root": "apps/meme-social",
      "entryFile": "main",
      "sourceRoot": "apps/meme-social/src",
      "compilerOptions": {
        "tsConfigPath": "apps/meme-social/tsconfig.app.json"
      }
    },
    "test": {
      "type": "application",
      "root": "apps/test",
      "entryFile": "main",
      "sourceRoot": "apps/test/src",
      "compilerOptions": {
        "tsConfigPath": "apps/test/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/meme-social"
}